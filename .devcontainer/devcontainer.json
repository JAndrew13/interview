{
	"name": "C# (.NET)",
	"image": "mcr.microsoft.com/devcontainers/dotnet:1-8.0",
	"features": {
		"ghcr.io/devcontainers/features/dotnet:2": {}
	},
	"forwardPorts": [
		5000,
		5001
	],
	"portsAttributes": {
		"5000": {
			"protocol": "http"
		},
		"5001": {
			"protocol": "https"
		}
	},
	"postCreateCommand": "dotnet restore",
}